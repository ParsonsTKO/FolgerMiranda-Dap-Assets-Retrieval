{
    "name": "ptko/dap/assets/retrieval",
    "type": "project",
    "license": "MIT",
    "description": "A project for retrieving external assets based on jobs from an SQS queue",
    "require": {
        "php": "~7.2.0",
        "ext-simplexml": "*",
        "aws/aws-sdk-php": "^3.55",
        "psr/container": "^1.0",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/debug": "^4.0",
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
